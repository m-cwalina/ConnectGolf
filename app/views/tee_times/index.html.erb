<%= week_calendar(events: @tee_times) do |date, tee_times| %>
  <%= date.day %>

  <% tee_times.map do |tee_time| %>
    <div>
      <h2><%= tee_time.time.strftime('%I:%M%p') %></h2>
      <% tee_time.bookings.map do |booking| %>
        <h2><%= booking.size %></h2>
      <% end %>
      <%=link_to new_tee_time_booking_path(tee_time) do %>
        <div type="button" class="btn btn-primary btn-lg">Tee Time</div>
      <% end %>
    </div>
  <% end %>
<% end %>
